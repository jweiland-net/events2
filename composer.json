{
  "name": "jweiland/events2",
  "type": "typo3-cms-extension",
  "description": "Organize your single and recurring events",
  "license": "GPL-2.0+-or-later",
  "keywords": ["typo3", "TYPO3 CMS", "event", "events2"],
  "homepage": "http://www.jweiland.net",
  "authors": [
    {
      "name": "Stefan Froemken",
      "email": "sfroemken@jweiland.net",
      "role": "Lead Developer"
    }
  ],
  "support": {
    "email": "projects@jweiland.net",
    "issues": "https://github.com/jweiland-net/events2/issues",
    "source": "https://github.com/jweiland-net/events2"
  },
  "require": {
    "typo3/cms-core": "^7.6.13 || ~8.5",
    "sjbr/static-info-tables": "~6.5",
    "jweiland/maps2": "~3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "nimut/testing-framework": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "JWeiland\\Events2\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JWeiland\\Events2\\Tests\\": "Tests"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/events2 ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/events2"
    ]
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
