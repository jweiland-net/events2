							<label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:chooseAction</label>
							<config>
								<type>select</type>
								<renderType>selectSingle</renderType>
								<items>
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:list</numIndex>
										<numIndex index="1" />
									</numIndex>
									<numIndex index="1">
										<numIndex index="0">LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:listLatest</numIndex>
										<numIndex index="1"><![CDATA[Day->listLatest;Day->show;Day->showByTimestamp;Location->show;Video->show]]></numIndex>
									</numIndex>
									<numIndex index="2">
										<numIndex index="0">LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:listToday</numIndex>
										<numIndex index="1"><![CDATA[Day->listToday;Day->show;Day->showByTimestamp;Location->show;Video->show]]></numIndex>
