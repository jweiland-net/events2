<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:sheetGeneral</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.listType>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:listType</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">
                                            LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:listType.list
                                        </numIndex>
                                        <numIndex index="1">list</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">
                                            LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:listType.listLatest
                                        </numIndex>
                                        <numIndex index="1">listLatest</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">
                                            LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:listType.listToday
                                        </numIndex>
                                        <numIndex index="1">listToday</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">
                                            LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:listType.listThisWeek
                                        </numIndex>
                                        <numIndex index="1">listWeek</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">
                                            LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:listType.listRange
                                        </numIndex>
                                        <numIndex index="1">listRange</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.listType>
                    <settings.pidOfDetailPage>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:pidOfDetailPage</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.pidOfDetailPage>
                    <settings.pidOfListPage>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:pidOfListPage</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.pidOfListPage>
                    <settings.pidOfLocationPage>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:pidOfLocationPage</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.pidOfLocationPage>
                    <settings.pidOfManagementPage>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:pidOfManagementPage</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.pidOfManagementPage>
                    <settings.categories>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:categories</label>
                            <config>
                                <type>select</type>
                                <renderType>selectTree</renderType>
                                <foreign_table>sys_category</foreign_table>
                                <renderMode>tree</renderMode>
                                <treeConfig>
                                    <parentField>parent</parentField>
                                    <rootUid>0</rootUid>
                                    <appearance>
                                        <showHeader>TRUE</showHeader>
                                        <expandAll>FALSE</expandAll>
                                        <maxLevels>99</maxLevels>
                                    </appearance>
                                </treeConfig>
                                <size>15</size>
                                <maxitems>99</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.categories>
                </el>
            </ROOT>
        </sDEF>
        <list>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:sheetList</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.latest.amountOfRecordsToShow>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:amountOfRecordsToShow
                            </label>
                            <config>
                                <type>input</type>
                                <eval>int</eval>
                                <size>5</size>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.latest.amountOfRecordsToShow>
                    <settings.mergeEventsAtSameDay>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:mergeEventsAtSameDay</label>
                            <description>
                                LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:mergeEventsAtSameDay.description
                            </description>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.mergeEventsAtSameDay>
                    <settings.mergeRecurringEvents>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:mergeRecurringEvents</label>
                            <description>
                                LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:mergeRecurringEvents.description
                            </description>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.mergeRecurringEvents>
                    <settings.preFilterByOrganizer>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:preFilterByOrganizer</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <foreign_table>tx_events2_domain_model_organizer</foreign_table>
                                <foreign_table_where>AND (###PAGE_TSCONFIG_ID### = 0 OR
                                    tx_events2_domain_model_organizer.pid = ###PAGE_TSCONFIG_ID###) ORDER BY
                                    tx_events2_domain_model_organizer.organizer
                                </foreign_table_where>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0"/>
                                        <numIndex index="1"/>
                                    </numIndex>
                                </items>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.preFilterByOrganizer>
                    <settings.showFilterForOrganizerInFrontend>
                        <TCEforms>
                            <label>
                                LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:showFilterForOrganizerInFrontend
                            </label>
                            <description>
                                LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:showFilterForOrganizerInFrontend.description
                            </description>
                            <config>
                                <type>check</type>
                                <renderType>checkboxToggle</renderType>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.showFilterForOrganizerInFrontend>
                </el>
            </ROOT>
        </list>
        <detail>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:sheetDetail</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.display>
                        <TCEforms>
                            <label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:display</label>
                            <description>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:display.description
                            </description>
                            <config>
                                <type>radio</type>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">
                                            LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:displayBoth
                                        </numIndex>
                                        <numIndex index="1">displayBoth</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">
                                            LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:displayImages
                                        </numIndex>
                                        <numIndex index="1">displayImages</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">
                                            LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:displayEvent
                                        </numIndex>
                                        <numIndex index="1">displayEvent</numIndex>
                                    </numIndex>
                                </items>
                                <default>displayBoth</default>
                            </config>
                        </TCEforms>
                    </settings.display>
                </el>
            </ROOT>
        </detail>
    </sheets>
</T3DataStructure>
