										<numIndex index="1"><![CDATA[Day->listThisWeek;Day->show;Day->showByTimestamp;Location->show;Video->show]]></numIndex>
									</numIndex>
									<numIndex index="4">
										<numIndex index="0">LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:listRange</numIndex>
										<numIndex index="1"><![CDATA[Day->listRange;Day->show;Day->showByTimestamp;Location->show;Video->show]]></numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</switchableControllerActions>
					<settings.pidOfDetailPage>
						<TCEforms>
							<label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:pidOfDetailPage</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</settings.pidOfDetailPage>
					<settings.pidOfListPage>
						<TCEforms>
							<label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:pidOfListPage</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</settings.pidOfListPage>
					<settings.pidOfLocationPage>
						<TCEforms>
							<label>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:pidOfLocationPage</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
