							<description>LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:display.description</description>
							<config>
								<type>radio</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">LLL:EXT:events2/Resources/Private/Language/FlexForms.xlf:displayBoth</numIndex>
